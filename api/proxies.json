{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "create_entry": {
            "matchCondition": {
                "route": "/api/create_entry"
            },
            "backendUri": "%WEB_HOST%/api/create_entry"
        },
        "read_entries": {
            "matchCondition": {
                "route": "/api/read_entries"
            },
            "backendUri": "%WEB_HOST%/api/read_entries"
        }
    }
}